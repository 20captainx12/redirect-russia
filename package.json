  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pabio/redirect-russia/issues"
  },
  "homepage": "https://redirectrussia.org",
  "devDependencies": {
    "esbuild": "^0.14.24",
    "parcel": "^2.3.2"
  }
}
